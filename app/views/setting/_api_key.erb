<div role="tabpanel" class="tab-pane<%if @in == 'api'%> active<%end%>" id="api">
		<div class="panel-body">
			<div data-example-id="simple-table" class="bs-example">

				<div class="panel panel-default">
					<div class="panel-heading"><b>My API Key</b></div>
					<div class="panel-body">

						<div class="form-group">
								<%= me_api_key %>
						</div>

						<%= form_for @user, url: '/' + me.username + '/setting/key/generate' do |f| %>
							<div class="form-group">
								<%= div_for(@user, class: 'form-group') do %>
									<%= submit_tag 'Generate new', class: 'btn btn-primary' %>
								<%end%>
							</div>
						<% end %>

					</div>
				</div>
			</div>
		</div>
</div>
